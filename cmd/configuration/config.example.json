{
  "SERVER": {
    "SERVER_PORT": 3000,
    "READ_BUFFER_SIZE": 61440
  },
  "DB": {
    "DB_SSLMODE": "require",
    "DB_DNS": "postgresql://localhost",
    "DB_SSLROOTCERT_PATH": "../configuration/server-ca.pem",
    "DB_SSLKEY_PATH": "../configuration/client-key.pem",
    "DB_SSLCERT_PATH": "../configuration/client-cert.pem",
    "DB_NAME": "your_database_name",
    "DB_PASSWORD": "your_database_password",
    "DB_USER": "your_database_user",
    "DB_PORT": "5432",
    "DB_HOST": "your_database_host"
  },
  "CORS": {
    "ALLOW_ORIGINS": "http://localhost:5173,http://localhost:4200",
    "ALLOW_METHODS": "GET,POST,PUT,DELETE,OPTIONS",
    "ALLOW_HEADERS": "Origin, Content-Type, Accept, Authorization",
    "ALLOW_CREDENTIALS": true
  },
  "GCS_SIGNED_URL": {
    "GCS_ENABLED": true,
    "GCS_BUCKET_NAME": "your_bucket_name",
    "GCS_SERVICE_KEY": "/path/to/service_key.json",
    "GCS_URL_EXPIRY_SEC": 3600,
    "GCS_SIGNED_URL_API_KEY": "your_api_key",
    "GCS_SIGNED_URL_SERVICE_NAME": "your_service_name",
    "GCS_SIGNED_URL_UPLOAD": "https://your-upload-url",
    "GCS_SIGNED_URL_DOWNLOAD": "https://your-download-url",
    "GCS_SIGNED_URL_EXPIRED": 45,
    "GCS_SIGNED_URL_BUCKET_NAME": "your_signed_bucket_name",
    "GCS_SIGNED_URL_PATH": "your_signed_url_path"
  },
  "SWAGGER": {
    "ENABLED": true
  },
  "BUSINESS_LOGIC": {
    "MAX_CANCEL_PER_CLASS": 2,
    "DAYS_BEFORE_CLASS_START_FOR_CANCELLATION": 7,
    "MAX_CAPACITY_PER_SESSION": 500
  },
  "LOGGER": {
    "ENABLED": true
  }
}